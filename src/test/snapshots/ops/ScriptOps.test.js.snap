    "c4f22465-2368-4e27-8013-e6399974fd48": {
      "_id": "c4f22465-2368-4e27-8013-e6399974fd48",
      "context": "SAML2_IDP_ATTRIBUTE_MAPPER",
      "createdBy": "null",
      "creationDate": 0,
      "default": true,
      "description": "Default global script for SAML2 IDP Attribute Mapper",
      "evaluatorVersion": "1.0",
