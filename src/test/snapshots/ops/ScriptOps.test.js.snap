    "6aa47a41-af3d-4df3-a7a1-b1766cfeaf6b": {
      "_id": "6aa47a41-af3d-4df3-a7a1-b1766cfeaf6b",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
      "createdBy": "null",
      "creationDate": 0,
      "default": false,
      "description": "null",
      "evaluatorVersion": "1.0",
      "language": "JAVASCRIPT",
      "lastModifiedBy": "null",
      "lastModifiedDate": 0,
      "name": "wks-NEO-Twillio",
      "script": [
        "/**",
        " * @file This script delivers a message with values from shared state via Twilio SMS, using the current user's telephoneNumber",
        " * @version 0.1.1",
        " * @keywords sms otp mfa twilio ",
        " */",
        "",
        "/**",
        " * Environment specific Twilio config ",
        " */",
        "",
        "var twilioAccountSid = "<secret>";   ",
        "var twilioApiKeySid = "<secret>";",
        "var twilioApiKeySecret = "<secret>";",
        "var twilioMessagingServiceSid = "<secret>";",
        "var twilioPhoneNumberFrom = "<secret>";    ",
        "",
        "/**",
        " * Full Configuration ",
        " */",
        "",
        "var config = {",
        "    accountSid: twilioAccountSid,    ",
        "    apiKeySid: twilioApiKeySid,",
        "    apiKeySecret: twilioApiKeySecret,",
        "    phoneNumberFrom: twilioPhoneNumberFrom, ",
        "    messagingServiceSid: twilioMessagingServiceSid, ",
        "    baseUri: "https://api.twilio.com/2010-04-01",",
        "    sharedStateIdField: "_id",",
        "    phoneNumberField: "telephoneNumber",",
        "    messageTemplate: "Your security code is {{oneTimePassword}}",",
        "    nodeName: "twilioSms"",
        "};",
        "",
        "/**",
        " * Node outcomes",
        " */",
        "",
        " var NodeOutcome = {",
        "    DELIVERED:  "delivered",",
        "    AUTHFAILED: "authFailed",",
        "    NOPHONE:    "noPhone",",
        "    ERROR:      "error"",
        "};",
        "",
        "/**",
        " * External dependencies",
        " */",
        "",
        "var fr = JavaImporter(",
        "    org.forgerock.util.encode.Base64,",
        "    java.lang.String",
        ");",
        "",
        "/**",
        " * Add the node info prefix to a log message",
        " * @param {string} message - the message body",
        " * @returns a tagged version of the message",
        " */",
        "function tag(message) {",
        "    return "***".concat(config.nodeName).concat(" ").concat(message);",
        "}",
        "",
        "/**",
        " * Log an HTTP response",
        " * ",
        " * @param {Response} HTTP response object",
        " */",
        "function logResponse(response) {",
        "    logger.message(tag("Scripted Node HTTP Response: " + response.getStatus() + ", Body: " + response.getEntity().getString()));",
        "}",
        "",
        "/**",
        " * Resolve handlebar placeholders from shared state",
        " * ",
        " * @param {template} handlebar template",
        " */",
        "function resolvePlaceholders(template) {",
        "  const regex = /{{.*?}}/g;",
        "  const variables = template.match(regex);",
        "",
        "  if (!variables) {",
        "    return template;",
        "  }",
        "  ",
        "  var result = template;",
        "  ",
        "  variables.forEach(function sub(element) { ",
        "    var varName = element.replace(/[{}]/g,"");",
        "    result = result.replace(element,sharedState.get(varName));",
        "  });",
        "  ",
        "  return result;",
        "}",
        "",
        "/**",
        " * Send an SMS via Twilio",
        " * ",
        " * @param {string} message - The text of the message to send",
        " * @param {string} numberFrom - The originating phone number/shortcode",
        " * @param {string} numberTo - The phone number to send to",
        " * @param {string} messagingServiceSid - Twilio messaging service SID",
        " * @param {strig} uri - The URI of the Twilio API endpoint ",
        " * @param {string} sid - The Twilio account or API key SID",
        " * @param {*} secret - The Twilio account authkey or API key secret",
        " * @returns - A node outcome ",
        " */",
        "function sendSMS(message, numberFrom, numberTo, messagingServiceSid, uri, sid, secret) {",
        "",
        "    logger.message(tag("Calling Twilio endpoint " + uri));",
        "    logger.message(tag("Sending to " + numberTo));",
        "",
        "    // Twilio seems not to expect the form values to be URL encoded",
        "",
        "    const requestBody = "Body=".concat(encodeURIComponent(message))",
        "                        .concat("&From=").concat(numberFrom)  ",
        "                        .concat("&To=").concat(numberTo)",
        "                        .concat("&MessagingServiceSid=").concat(messagingServiceSid);",
        "  ",
        "    logger.message(tag("Message body " + requestBody));",
        "",
        "    const basicAuth = fr.Base64.encode(fr.String(sid.concat(":").concat(secret)).getBytes());",
        "    ",
        "    var response;",
        "",
        "    try {",
        "        var request = new org.forgerock.http.protocol.Request();",
        "        request.setMethod("POST");",
        "        request.setUri(uri);",
        "        request.getHeaders().add("Authorization","Basic " + basicAuth);",
        "        request.getHeaders().add("Content-Type","application/x-www-form-urlencoded");",
        "        request.getEntity().setString(requestBody);",
        "",
        "        response = httpClient.send(request).get();",
        "    }",
        "    catch (e) {",
        "        logger.error(tag("Exception calling twilio endpoint " + e));",
        "        return NodeOutcome.ERROR;",
        "    }",
        "",
        "    logResponse(response);",
        "    const status = response.getStatus().getCode();",
        "",
        "    if (status === 401) {",
        "        logger.error(tag("Twilio authentication failed"));",
        "        return NodeOutcome.AUTHFAILED;    ",
        "    }",
        "",
        "    if (status !== 201) {",
        "        logger.error(tag("Bad HTTP response from Twilio: " + status));",
        "        return NodeOutcome.ERROR;    ",
        "    }",
        "",
        "    var responseJson = response.getEntity().getString();",
        "",
        "    if (!responseJson) {",
        "        logger.error(tag("No response body"));",
        "        return NodeOutcome.ERROR;    ",
        "    }",
        "",
        "    var responseObj = JSON.parse(responseJson);",
        "",
        "    if (!responseObj) {",
        "        logger.error(tag("Couldn't parse response body"));",
        "        return NodeOutcome.ERROR;    ",
        "    }",
        "",
        "    if (!(responseObj.hasOwnProperty("sid"))) {",
        "        logger.error(tag("No sid property in Twilio response"));",
        "        return NodeOutcome.ERROR;",
        "    }  ",
        "",
        "    logger.message(tag("message delivered successfully"));",
        "    return NodeOutcome.DELIVERED;",
        "}",
        "",
        "/**",
        " * Node entry point",
        " */",
        "",
        "logger.message(tag("node executing"));",
        "",
        "var id;",
        "var telephoneNumbers;",
        "var numberTo;",
        "",
        "if (!(id = sharedState.get(config.sharedStateIdField))) {",
        "    logger.error(tag("No id in shared state"));",
        "    outcome = NodeOutcome.ERROR;",
        "}",
        "else if (!(telephoneNumbers = idRepository.getAttribute(id, config.phoneNumberField))) {",
        "    logger.error(tag("No phone numbers for user"));",
        "    outcome = NodeOutcome.NOPHONE;",
        "}",
        "else if (!(numberTo = telephoneNumbers.toArray()[0])) {",
        "    logger.error(tag("no telephoneNumber"));",
        "    outcome = NodeOutcome.NOPHONE;",
        "}",
        "else if (numberTo === "") {",
        "    logger.error(tag("blank telephoneNumber"));",
        "    outcome = NodeOutcome.NOPHONE;",
        "}",
        "else {",
        "    var message = resolvePlaceholders(config.messageTemplate);",
        "    var twilioUri = config.baseUri.concat("/Accounts/").concat(config.accountSid).concat("/Messages.json");",
        "    outcome = sendSMS(message, config.phoneNumberFrom, numberTo, config.messagingServiceSid, twilioUri, config.apiKeySid, config.apiKeySecret);",
        "}",
      ],
    },
