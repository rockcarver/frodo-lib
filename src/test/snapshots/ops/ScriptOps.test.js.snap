        "}",
        "",
        "function getStates(data, c) {",
        "  var s = [];",
        "  data[c].states.forEach(e=>s.push(e.name));",
        "  return s;",
        "}",
        "",
        "function getCities(data, c, s) {",
        "  var ci = [];",
        "  data[c].states[s].cities.forEach(e=>ci.push(e.name));",
        "  return ci;",
        "}",
