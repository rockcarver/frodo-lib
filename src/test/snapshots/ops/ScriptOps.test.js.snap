      "name": "selectFavFactor.js",
      "script": [
        "// Displays MFA options filtered by users preferences",
        "var webauthnDeviceProfiles = idRepository.getAttribute(sharedState.get("_id"), "webauthnDeviceProfiles").toString();",
        "var pushDeviceProfiles = idRepository.getAttribute(sharedState.get("_id"), "pushDeviceProfiles").toString();",
        "",
        "var hasWebAuthN=false;",
        "var hasPush=false;",
        "",
        "if(webauthnDeviceProfiles.length>2) {",
        "    hasWebAuthN = true;",
        "}",
        "if(pushDeviceProfiles.length>2) {",
        "    hasPush = true;",
        "}",
        "",
        "",
        "mfas = { ",
        "          "Push": "Push notification via authenticator app", ",
        "         "WebAuthN": "Security Key/TouchID/WindowsHello/PassKey", ",
        "           //"QR" : "QR Code Cascaded Context (WebAuthN)",",
        "           "SMS" : "SMS Cascaded Context (WebAuthN)",",
        "          //"Email" : "E-mail Cascaded Context (WebAuthN)",",
        "          // "SMSML" : "SMS Magic Link"",
        "       };",
        "",
        "",
        "var fr = JavaImporter(",
        "      org.forgerock.openam.auth.node.api.Action,",
        "      javax.security.auth.callback.TextOutputCallback,",
        "      javax.security.auth.callback.ChoiceCallback",
        ")",
        "",
        "var username = sharedState.get("_id");",
        "",
        "// Add entry to multi value",
        "var choices = [];",
        "if(hasPush==true){",
        "      choices.push(mfas.Push);",
        "}",
        "if(hasWebAuthN==true){",
        "      choices.push(mfas.WebAuthN);",
        "      //choices.push(mfas.QR);",
        "      //choices.push(mfas.SMS);",
        "    //choices.push(mfas.Email);",
        "",
        "}",
        "//choices.push(mfas.SMSML);",
        "function setCustomAttrValue (customAttrName,customAttrValue)",
        "{",
        "  var id = sharedState.get('_id');",
        "  var frIdmCustomAttributes = idRepository.getAttribute(id, 'fr-idm-custom-attrs');",
        "  if (frIdmCustomAttributes)",
        "  {",
        "    var frIdmCustomAttributesObject = JSON.parse(String(frIdmCustomAttributes.toArray()[0]));",
        "    frIdmCustomAttributesObject[customAttrName] = customAttrValue;",
        "    var json_string = JSON.stringify(frIdmCustomAttributesObject);",
        "    idRepository.setAttribute(id, "fr-idm-custom-attrs",[json_string]);",
        "  }",
        "}",
        "",
        "",
        "//Callback to present and retrieve selected org and set",
        "//selected theme for setting in the next node",
        "",
        "",
        "",
        "with (fr) {",
        "      if (choices.length < 2) action = Action.goTo(outcome).build();",
        "    else {",
        "         if (callbacks.isEmpty()) {",
        "              action = fr.Action.send(",
        "                new fr.ChoiceCallback(",
        "                      "Strong Authentication Required. Please select your preferred form of strong authentication from the list below.",",
        "                  choices,",
        "                  0,",
        "                  false",
        "                )",
        "              ).withStage(JSON.stringify({ "ChoiceCallback": [ { "displayType":"radio" } ] })).withHeader("Choose Method").build();",
        "          } else {",
        "            var selectedIndex = callbacks.get(0).getSelectedIndexes()[0];",
        "            var selectedName;",
        "            for (var key in mfas) if (choices[selectedIndex] == mfas[key]) selectedName = key;",
        "            sharedState.put("selectedName", selectedName);",
        "              setCustomAttrValue ("custom_favouriteFactor",selectedName);",
        "            action = Action.goTo(selectedName).build();",
        "        }   ",
        "    }",
        "}",
      ],
