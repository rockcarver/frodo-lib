        "            const p = document.querySelectorAll('input[data-vv-as=\\"DUMMY\\"]')[0]; \\n\\",
        "            if (p) { \\n\\",
        "                p.hidden = true; \\n\\",
        "                document.querySelector('label[for=' + p.id + ']').hidden = true; \\n\\",
        "                document.body.querySelector('button[type=submit]').innerText = '" + BUTTONTEXT + "'; \\n\\",
        "                var b = document.createElement('button'); \\n\\",
        "                b.classList.add(\\"btn\\", \\"mt-3\\", \\"btn-secondary\\"); \\n\\",
        "                b.innerHTML = '" + MESSAGE + "' + COUNT + 's'; \\n\\",
        "                b.onclick = function() { p.value='___TIME___'; p.dispatchEvent(new Event('input')); }; \\n\\",
        "                p.parentNode.insertBefore(b, p.nextSibling); \\n\\",
        "                var t = setInterval(function() { \\n\\",
        "                    if (COUNT == 1) { \\n\\",
        "                        clearInterval(t); \\n\\",
        "                        p.value='___TIME___'; p.dispatchEvent(new Event('input')); \\n\\",
        "                        setTimeout(function() { document.getElementById('loginButton_0').click(); }, 200); \\n\\",
        "                    } else { \\n\\",
        "                        COUNT--; \\n\\",
        "                        b.innerHTML = '" + MESSAGE + "' + COUNT + 's'; \\n\\",
        "                    } \\n\\",
        "                }, 1000 ); \\n\\",
        "                if (obs) obs.disconnect(); \\n\\",
        "                return; \\n\\",
        "            } \\n\\",
        "        } \\n\\",
        "        if (document.querySelectorAll('input[data-vv-as=\\"DUMMY\\"]')[0]) go(); \\n\\",
        "        else { \\n\\",
        "            const observer = new MutationObserver((mutations, obs) => { go(obs); }); \\n\\",
        "            observer.observe(document, { childList: true, subtree: true }); \\n\\",
        "        } \\n\\",
        "    ");",
        "}",
        "",
        "",
        "if (callbacks.isEmpty()) {",
