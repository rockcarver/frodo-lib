    "da56fe60-8b38-4c46-a405-d6b306d4b336": {
      "_id": "da56fe60-8b38-4c46-a405-d6b306d4b336",
      "context": "OAUTH2_EVALUATE_SCOPE",
      "createdBy": "null",
      "creationDate": 0,
