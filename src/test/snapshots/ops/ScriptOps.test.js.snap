      "script": [
        "var attribute = "fr-attr-str2";",
        "var threshold = 2;",
        "outcome = "default";",
        "var now = new Date()  ",
        "var epoch = Math.round(now.getTime() / 1000);",
        "var epochCurrentMinute = Math.round(epoch/60);",
        "",
        "var userAttr;",
        "var _id = sharedState.get("_id");",
        "var epochAttrMinute=0;",
        "var attemptCount=1;",
        "",
        "var userPreferences = idRepository.getAttribute(_id, "fr-idm-preferences").toString();",
        "",
        "if(userPreferences.indexOf("\\"custom_push\\":true") > -1)",
        "{",
        "    outcome = "suspended";",
        "}",
        "else",
        "{",
        "  var userAttrRaw = idRepository.getAttribute(_id, attribute);",
        "  if (userAttrRaw && userAttrRaw.iterator().hasNext()) {",
        "      userAttr = JSON.parse(userAttrRaw.iterator().next().toString());",
        "      epochAttrMinute = userAttr[0];",
        "      attemptCount = userAttr[1];",
        "      attemptCount = attemptCount + 1;",
        "  }",
        "",
        "  //if(",
        "",
        "  var newAttr;",
        "  if(epochCurrentMinute==epochAttrMinute) {",
        "      newAttr = String("[" + epochAttrMinute.toString() + "," + attemptCount.toString()  + "]");",
        "  }",
        "  else {",
        "      attemptCount=1;",
        "      newAttr = String("[" + epochCurrentMinute.toString() + "," + attemptCount.toString() + "]");",
        "  }",
        "  idRepository.setAttribute(_id, "fr-attr-str2",[newAttr]);",
        "",
        "  if(attemptCount>threshold) {",
        "    outcome = "true";",
        "  }",
        "  else {",
        "    outcome = "false";",
        "  }",
        "}",
      ],
    },
