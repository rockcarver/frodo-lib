    "3c9d3b4a-b38c-4c77-a8d7-4b937e48f62f": {
      "_id": "3c9d3b4a-b38c-4c77-a8d7-4b937e48f62f",
      "context": "AUTHENTICATION_TREE_DECISION_NODE",
