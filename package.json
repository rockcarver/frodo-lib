    "rimraf": "^6.0.1",
    "setup-polly-jest": "^0.11.0",
    "slugify": "^1.6.5",
    "sshpk": "^1.18.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typedoc": "^0.28.16",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "5.8",
    "uuid": "^11.1.0",
    "winston": "^3.19.0"
  }
}
